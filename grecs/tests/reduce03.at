AT_SETUP([Reduction: table driven])
AT_KEYWORDS([reduce reduce03])

GRECS_TEST([
scalar bar;
listvar (1, 2);
listvar foo;
logging {
  facility local1;
  tag prog;
}
listvar (3, 4);
scalar baz;
logging {
  facility mail;
}
],
[gcfset -reduce],
[0],
[Global settings:
scalar = baz
listvar = "1" "2" "foo" "3" "4"
logging: 0/mail/prog/0
])

AT_CLEANUP
